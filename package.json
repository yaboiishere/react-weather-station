  "private": true,
  "homepage": " https://yaboiishere.github.io/react-weather-station/",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/styles": "^4.11.3",
    "@types/react": "16.9.23",
    "actioncable": "^5.2.4-5",
    "axios": "^0.20.0",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "gh-pages": "^3.1.0",
    "history": "4.10.1",
    "leaflet": "^1.7.1",
    "node-sass": "4.13.1",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "15.7.2",
    "react": "^17.0.1",
    "react-chartjs-2": "2.9.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.1.0",
    "react-notification-alert": "0.0.12",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "reactstrap": "8.4.1",
    "underscore": "^1.12.0",
    "universal-cookie": "^4.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-scss": "node-sass src/assets/scss/black-dashboard-react.scss src/assets/css/black-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/black-dashboard-react.scss src/assets/css/black-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/black-dashboard-react.scss src/assets/css/black-dashboard-react.css --source-map true",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "@types/googlemaps": "3.39.3",
    "@types/markerclustererplus": "2.1.33",
    "eslint-plugin-flowtype": "3.13.0",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8"
  }
}
